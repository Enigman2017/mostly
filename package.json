{
  "name": "mostly",
  "version": "1.0.0",
  "description": "They mostly come at night; mostly",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "sh ./scripts/dev.sh",
    "dev:client": "sh ./scripts/devClient.sh",
    "dev:server": "sh ./scripts/devServer.sh",
    "test": "sh ./scripts/test.sh",
    "test:unit": "sh ./scripts/testUnit.sh",
    "test:func": "sh ./scripts/testFunc.sh",
    "test:e2e": "sh ./scripts/testE2E.sh",
    "lint": "sh ./scripts/lint.sh",
    "build": "sh ./scripts/build.sh",
    "deploy": "sh ./scripts/deploy.sh",
    "start": "sh ./scripts/prod.sh",
    "clean": "sh ./scripts/clean.sh",
    "precommit": "sh ./scripts/precommit.sh"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "axios": "0.18.0",
    "express": "4.16.3",
    "helmet": "3.13.0",
    "morgan": "1.9.0",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.4",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.55",
    "@babel/core": "7.0.0-beta.55",
    "@babel/node": "7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.55",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "7.0.0-beta.55",
    "@babel/preset-env": "7.0.0-beta.55",
    "@babel/preset-react": "7.0.0-beta.55",
    "@intervolga/optimize-cssnano-plugin": "1.0.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "8.0.0-beta.4",
    "chokidar": "2.0.4",
    "concurrently": "3.6.1",
    "css-loader": "1.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "23.4.2",
    "jest-puppeteer": "3.2.1",
    "lint-staged": "7.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "now": "11.3.2",
    "prettier": "1.14.0",
    "puppeteer": "1.6.1",
    "style-loader": "0.21.0",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  },
  "config": {
    "registry": "https://registry.npmjs.org/"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime",
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "airbnb",
    "globals": {
      "beforeAll": true,
      "beforeEach": true,
      "describe": true,
      "document": true,
      "expect": true,
      "it": true,
      "jest": true,
      "page": true,
      "test": true,
      "window": true
    }
  },
  "eslintIgnore": [
    "coverage/**",
    "dist/**"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --ignore-pattern /dist/ --fix ./",
      "git add"
    ],
    "*.{css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
